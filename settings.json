{
    "theme": "blue",
    "logFile": "player.log",
    "logLevel": "DEBUG",
    "debug": true,
    "proxyServerPort": 5000,

    "visualization": "Column equalizer",

    "keybindings": [
        {
            "keys": ["space"],
            "command": "togglePause"
        },
        {
            "keys": ["arrowleft"],
            "command": "seekBy",
            "args": [-20]
        },
        {
            "keys": ["arrowright"],
            "command": "seekBy",
            "args": [20]
        },
        {
            "keys": ["ctrl+arrowleft"],
            "command": "seekBy",
            "args": [-120]
        },
        {
            "keys": ["ctrl+arrowright"],
            "command": "seekBy",
            "args": [120]
        },
        {
            "keys": ["p"],
            "command": "skipTrackBy",
            "args": [-1]
        },
        {
            "keys": ["n"],
            "command": "skipTrackBy",
            "args": [1]
        },
        {
            "keys": [";", "ctrl+shift+p"],
            "command": "openCommandsPalette"
        },
        {
            "keys": ["+"],
            "command": "volumeBy",
            "args": [0.05]
        },
        {
            "keys": ["-"],
            "command": "volumeBy",
            "args": [-0.05]
        },
        {
            "keys": ["ctrl+p"],
            "command": "playlist.open"
        },
        {
            "keys": ["ctrl+n"],
            "command": "playlist.create"
        },
        {
            "keys": ["shift+delete"],
            "command": "playlist.delete"
        },
        {
            "keys": ["ctrl+shift+r"],
            "command": "playlist.rename"
        },
        {
            "keys": ["r"],
            "command": "playlist.randomTrack"
        },
        {
            "keys": ["escape"],
            "command": "blur"
        },
        {
            "keys": ["arrowdown"],
            "command": "navigation.down"
        },
        {
            "keys": ["arrowup"],
            "command": "navigation.up"
        },
        {
            "keys": ["pagedown"],
            "command": "navigation.pageDown"
        },
        {
            "keys": ["pageup"],
            "command": "navigation.pageUp"
        },
        {
            "keys": ["shift+arrowdown"],
            "command": "navigation.addToSelectionDown"
        },
        {
            "keys": ["shift+arrowup"],
            "command": "navigation.addToSelectionUp"
        },
        {
            "keys": ["shift+pagedown"],
            "command": "navigation.addPageToSelectionDown"
        },
        {
            "keys": ["shift+pageup"],
            "command": "navigation.addPageToSelectionUp"
        },
        {
            "keys": ["enter"],
            "command": "navigation.selectItem"
        },
        {
            "keys": ["delete"],
            "command": "navigation.deleteItem"
        },
        {
            "keys": ["ctrl+w", "x"],
            "command": "panes.closeView"
        },
        {
            "keys": ["ctrl+pageup"],
            "command": "panes.switchViewBy",
            "args": [-1]
        },
        {
            "keys": ["ctrl+pagedown"],
            "command": "panes.switchViewBy",
            "args": [1]
        },
        {
            "keys": ["ctrl+f", "/"],
            "command": "panes.searchContent"
        },
        {
            "keys": ["v"],
            "command": "visualization.toggle"
        },
        {
            "keys": ["ctrl+shift+v"],
            "command": "visualization.change"
        },
        {
            "keys": ["shift+?"],
            "command": "shortcuts.open"
        },
        {
            "keys": ["ctrl+shift+f"],
            "command": "search.search"
        },
        {
            "keys": ["ctrl+]"],
            "command": "notificationsPlayground.spawnNotification"
        },
        {
            "keys": ["ctrl+r"],
            "command": "trackScheduler.randomOrder"
        },
        {
            "keys": ["ctrl+o"],
            "command": "trackScheduler.oneByOneOrder"
        }
    ]
}
